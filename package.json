  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently --kill-others  \"webpack-dev-server --open\" \"npm run watch\"",
    "server": "node server.js",
    "webpack:build": "webpack --env.NODE_ENV=local --env.development --config webpack.config.js",
    "webpack:watch": "npm run webpack:build -- --watch --debug",
    "build": "npm run webpack:build",
    "watch": "concurrently \"npm run webpack:watch\"",
    "release:prepare": "node ./scripts/release-prepare.js",
    "release:publish": "node ./scripts/release-publish.js",
    "web:clean": "rimraf ../com-buddy-delaune-build/www/",
    "web:copy": "cp -R dist/ ../com-buddy-delaune-build/www/",
    "web:release": "npm run web:clean && npm run web:copy && node ./scripts/release-build.js --buildPath=\"../com-buddy-delaune-build\"",
    "bump": "npm run release:prepare -- --release-as patch && npm run build && npm run release:publish && npm run web:release"
  },
